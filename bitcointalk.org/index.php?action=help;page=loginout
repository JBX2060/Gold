	var sites = new Array();sites["https://www.privateinternetaccess.com/"] = 1;
sites["https://bitcointalk.org/dec/p1.html"] = 2;
sites["https://fundyourselfnow.com"] = 3;
sites["https://bountyhive.io"] = 4;
