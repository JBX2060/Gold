.browserupgrade{margin:0.2em 0;background:#ccc;color:#000;padding:0.2em 0;}.preview-header{margin-top:10px;}.preview-header-info{padding-top:5px;display:inline-block;}.preview-window{width:100%;height:calc(100% - 55px);position:absolute;top:55px;bottom:0;left:0;right:0;border:none;border-top:solid 5px lightgrey;}.auto-download{display:none;}.payment-modal .modal-body{padding:75px 0;}.payment-description{font-size:15px;}.price{font-size:55px;}.messenger{display:block;margin-top:15px;font-size:16px;}.pay-button-click-spinner{margin-right:10px;}.checkout-timer{position:absolute;top:17px;right:50px;display:none;}.modal-backdrop.in{opacity:0.8;}#wd-assets-tab-content .tab-pane{overflow-y:scroll;height:235px;padding:0 10px;}#wd-assets-tab-content ol{margin-left:10px;-webkit-padding-start:10px;padding-start:10px;}.badge{margin-left:10px;padding:4px 7px 4px;}.modal-body{position:relative;padding:0 15px 15px;}.modal-header{padding:15px 15px 0 15px;border-bottom:1px solid transparent;}.modal-footer{text-align:center;box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;border-top:2px solid #eff0f1;}#show-files-list-2{cursor:pointer;text-decoration:underline;}.back-wd-asset-list-link{padding:10px 20px 10px 10px;display:none;}.get-zip-btn-shaker{animation:shake 1s cubic-bezier(.36,.07,.19,.97) both;transform:translate3d(0,0,0);}@keyframes shake{10%,90%{transform:translate3d(-1px,0,0);}20%,80%{transform:translate3d(2px,0,0);}30%,50%,70%{transform:translate3d(-4px,0,0);}40%,60%{transform:translate3d(4px,0,0);}}